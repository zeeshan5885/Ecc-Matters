#!/bin/bash
newexec="${0%/lalapps_fftw_wisdom}/lal_fftw_wisdom"
echo >&2
echo "WARNING: 'lalapps_fftw_wisdom' has been renamed to 'lal_fftw_wisdom'" >&2
echo "WARNING: please update your scripts" >&2
echo >&2
if test ! -x "${newexec}"; then
  echo "ERROR: '${newexec}' is not installed" >&2
  echo >&2
  exit 127
fi
case "$1" in -h|--help) exit 1;; esac
echo "WARNING: now running '${newexec}' ..." >&2
echo >&2
exec "${newexec}" "$@"
exit 1
